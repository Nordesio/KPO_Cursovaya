@using X.PagedList.Mvc;
@using X.PagedList.Mvc.Core;
@using KPO_Cursovaya.Models;
@model X.PagedList.IPagedList<Request>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
<head>
    <link rel="stylesheet" href="~/css/PagedList.css">
</head>



@foreach(var image in Model){
    <img src="@image.Url" style="max-width: 30%; max-height: 30%;" alt="Image">
}
<div class = "themes">


    Страница @(Model.PageCount < Model.PageNumber ? 0 : Model.PageNumber) из @Model.PageCount
 
    @Html.PagedListPager(Model, page => Url.Action("VkResult", new { vk_id = ViewBag.Id, id = page }))

        
</div>